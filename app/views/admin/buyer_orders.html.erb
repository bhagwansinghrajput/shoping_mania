<table class="table-bordered table-hover" >
      <tr>
      <th>shipping_charge </th>
      <th> tax_amount </th>
      <th> discount_on_promocode </th>
      <th> net_amount </th>
      <th> status </th>
      <th> shipping_date </th>
    </tr>
    <% @orders_of_buyer.each do |order| %>
      <tr>
        <td><%= order.shipping_charge %></td>
        <td><%= order.tax_amount %></td>
        <td><%= order.discount_on_promocode %></td>
        <td><%= order.net_amount %></td>
        <td><%= order.status %></td>
        <td><%= order.shipping_date %></td>
      </tr>
    <% end %>
</table>